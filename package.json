{
  "name": "start-up-api",
  "version": "1.0.0",
  "description": "Node API for start-up",
  "scripts": {
    "start": "NODE_PATH=./build/src node build/src/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --ignore-watch node_modules --ignore-watch ./src/swagger.json ./src/server.ts",
    "build": "tsc",
    "gcp-build": "yarn generate-docs && yarn build",
    "lint": "eslint",
    "test": "yarn coverage",
    "test:plain": "cross-env NODE_ENV=test yarn run run-tests",
    "run-tests": "ts-mocha ./tests/**/*.test.ts -opts tests/mocha.opts",
    "coverage": "cross-env NODE_ENV=test nyc ts-mocha ./tests/**/*.test.ts -opts tests/mocha.opts",
    "load-fixtures": "ts-node -r tsconfig-paths/register ./scripts/loadFixtures",
    "load-fixtures:proxy": "cross-env NODE_ENV=proxy ts-node -r tsconfig-paths/register ./scripts/loadFixtures",
    "load-fixtures:production": "yarn install --production=false && yarn run load-fixtures",
    "clear-database": "ts-node -r tsconfig-paths/register ./scripts/clearDatabase",
    "generate-docs": "ts-node -r tsconfig-paths/register ./scripts/generateDocs",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./src/cliconfig.ts",
    "process-coverage": "ts-node -r tsconfig-paths/register ./scripts/processCoverage",
    "copy-assets": "ts-node ./scripts/copyAssets"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.2.0-preview.1",
    "@google-cloud/storage": "^3.3.1",
    "@sendgrid/mail": "^6.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.3",
    "@types/cls-hooked": "^4.3.0",
    "@types/express": "^4.17.1",
    "@types/fs-extra": "^9.0.1",
    "@types/helmet": "^0.0.44",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.5.0",
    "@types/sinon": "^7.5.0",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.1",
    "class-validator-jsonschema": "^1.3.1",
    "cls-hooked": "^4.2.2",
    "core-js": "^3.4.7",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "fs-extra": "^9.0.1",
    "hash-files": "^1.1.1",
    "helmet": "^3.21.1",
    "husky": "^3.0.9",
    "into-stream": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.2.1",
    "multer": "^1.4.2",
    "nyc": "^14.1.1",
    "pg": "^7.12.1",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.1",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.25",
    "typeorm-fixtures-cli": "^1.4.2",
    "typescript": "^3.9.5",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "eslint-import-resolver-typescript": "^2.0.0"
  }
}
